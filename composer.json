{
    "name": "ziffdavis/laravel-eloquent-imagery",
    "type": "library",
    "authors": [
        {
            "name": "Ralph Schindler",
            "email": "ralph_schindler@ziffdavis.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "laravel/laravel": "~5.5.0|~5.6.0",
        "intervention/image": "^2.4",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "ZiffDavis\\Laravel\\EloquentImagery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZiffDavis\\Laravel\\EloquentImagery\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZiffDavis\\Laravel\\EloquentImagery\\EloquentImageryProvider"
            ]
        }
    }
}
